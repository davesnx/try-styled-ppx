{
  "name": "try-styled-ppx",
  "jsx": {
    "version": 3
  },
  "sources": [
    {
      "dir": "source",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "@davesnx/styled-ppx/css",
    "@davesnx/styled-ppx/emotion"
  ],
  "ppx-flags": [["@davesnx/styled-ppx/ppx"]],
  "warnings": {
    "error": "+5"
  },
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"]
}
