	},
	"keywords": [
		"BuckleScript",
		"Reason",
		"ReasonReact"
	],
	"dependencies": {
		"@ahrefs/bs-emotion": "^1.0.1",
		"@ahrefs/bs-emotion-ppx": "^1.0.2-dev.3",
		"@davesnx/styled-ppx": "^1.0.1-nightly.7dafd84.0",
		"autoprefixer": "^9.8.0",
		"bs-css-emotion": "^2.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"reason-react": ">=0.8.0"
	},
	"devDependencies": {
		"@glennsl/bs-jest": "^0.5.1",
		"bs-css": "^13.0.0",
		"bs-platform": "^7.3.2",
		"bs-react-testing-library": "^0.7.2",
		"bs-webapi": "^0.15.9",
		"concurrently": "^5.2.0",
		"css-loader": "^3.5.3",
		"html-webpack-plugin": "^4.3.0",
		"mini-css-extract-plugin": "^0.9.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"style-loader": "^1.2.1",
		"terser-webpack-plugin": "^3.0.1",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"jest": {
		"roots": [
			"<rootDir>/tests"
		],
		"testMatch": [
			"<rootDir>/tests/**/*_test.{js,bs.js}"
		]
	},
	"scripts": {
		"build": "bsb -make-world && NODE_ENV=production webpack --config config/webpack.config.js && cp -n public/* build/ | true",
		"start": "concurrently -c --kill-others 'yarn start:bsb' 'yarn start:webpack'",
		"start:bsb": "bsb -make-world -w",
		"start:webpack": "webpack-dev-server --config config/webpack.config.js --open",
		"clean": "bsb -clean-world",
		"test": "bsb -make-world && jest",
		"format": "bsrefmt --in-place */**/*.re */*.re"
	}
}
